{
  "name": "@xpack/logger",
  "version": "3.0.1",
  "description": "A generic console logger class",
  "main": "./index.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "test": "./test"
  },
  "bin": {},
  "scripts": {
    "prepublishOnly": "standard && npm run test-tap100 -s",
    "test": "standard && npm run test-tap -s",
    "test100": "standard && npm run test-tap100 -s",
    "test-coverage": "tap --coverage --reporter=classic --timeout 600 \"test/tap/*.js\"",
    "test-tap": "tap --reporter=classic --timeout 300 \"test/tap/*.js\"",
    "test-tap100": "tap --reporter=classic --timeout 300 --100 \"test/tap/*.js\"",
    "tap": "tap --reporter=spec --timeout 300",
    "fix": "standard --fix --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpack/logger-js.git"
  },
  "keywords": [
    "cli",
    "logger",
    "log",
    "console",
    "level",
    "silent",
    "error",
    "warn",
    "info",
    "verbose",
    "debug",
    "trace"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpack/logger-js/issues/"
  },
  "homepage": "https://github.com/xpack/logger-js/",
  "devDependencies": {
    "standard": "^16.0.3",
    "tap": "^15.0.0"
  },
  "peerDependencies": {},
  "bundledDependencies": [],
  "optionalDependencies": {},
  "engines": {
    "node": ">10.0"
  }
}
